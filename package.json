{
  "name": "@haowjy/remote-workspace",
  "version": "0.1.3",
  "description": "Mobile-friendly web workspace for SSH + tmux workflows.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haowjy/remote-workspace.git"
  },
  "bugs": {
    "url": "https://github.com/haowjy/remote-workspace/issues"
  },
  "homepage": "https://github.com/haowjy/remote-workspace#readme",
  "type": "module",
  "scripts": {
    "dev": "tsx src/launcher.ts",
    "dev:server": "tsx src/server.ts",
    "build": "tsc -p tsconfig.json",
    "prepack": "npm run build",
    "start": "node dist/server.js"
  },
  "files": [
    "dist",
    "static",
    "README.md",
    "LICENSE*"
  ],
  "dependencies": {
    "express": "^4.21.2",
    "mime-types": "^2.1.35",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.16.5",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "bin": {
    "remote-workspace": "dist/launcher.js"
  },
  "publishConfig": {
    "access": "public"
  }
}
